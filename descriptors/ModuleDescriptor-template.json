{
  "id": "${artifactId}-${version}",
  "name": "Patron Services Edge API",
  "provides": [],
  "requires": [
    {
      "id": "patron",
      "version": "4.2"
    },
    {
      "id": "circulation",
      "version": "9.5 10.0 11.0"
    },
    {
      "id": "login",
      "version": "5.0 6.0 7.0"
    }
  ],
  "permissionSets": [],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 268435456,
        "PortBindings": { "8081/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      }
    ]
  }
}
